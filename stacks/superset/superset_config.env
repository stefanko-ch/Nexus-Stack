# Superset configuration environment variables
# This file contains additional configuration for Superset

# Disable example data loading (faster startup)
SUPERSET_LOAD_EXAMPLES=no

# Production settings
FLASK_DEBUG=false
SUPERSET_ENV=production
SUPERSET_LOG_LEVEL=info

# Feature flags
FEATURE_FLAGS={"ENABLE_TEMPLATE_PROCESSING": true, "ALERT_REPORTS": true}

# Disable telemetry
SCARF_ANALYTICS=false
